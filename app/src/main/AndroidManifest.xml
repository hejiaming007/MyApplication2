<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myapplication2.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.myapplication2.app.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.myapplication2.app.ResultActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.myapplication2.app.SettingsActivity"
            android:label="@string/title_activity_settings_activity2" >
        </activity>
        <activity
            android:name="com.example.myapplication2.app.Test_ScrollView"
            android:label="@string/title_activity_settings_activity2" >
        </activity>

        <service
            android:name="com.example.myapplication2.services.ServiceDemo"
            android:enabled="true"
            android:exported="true" >
            <intent-filter>
                <action android:name="com.example.myapplication2.service.ServiceDemo" />
            </intent-filter>
        </service>
    </application>

    <uses-permission android:name="android.permission.CALL_PHONE" />


</manifest>
